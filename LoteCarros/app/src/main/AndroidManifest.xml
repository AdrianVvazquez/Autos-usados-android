<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.contextmenu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Autos usados">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <!--        Uno por cada screen        -->
        <activity
            android:name="com.example.contextmenu.DetailScreen"
            android:label="@string/activity_details">
        </activity>

        <activity
            android:name="com.example.contextmenu.AddScreen"
            android:label="@string/add_car_screen">
        </activity>

        <activity
            android:name="com.example.contextmenu.ModifyScreen"
            android:label="@string/update_car_screen">
        </activity>


        <activity
            android:name="com.example.contextmenu.Error404Page"
            android:label="@string/error_404_string">
        </activity>

    </application>


</manifest>
